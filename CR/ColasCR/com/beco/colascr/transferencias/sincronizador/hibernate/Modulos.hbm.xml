<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.beco.colascr.transferencias.sincronizador.hibernate.Modulos"
    table="modulos"
>

    <id
        name="codmodulo"
        type="int"
        column="codmodulo"
    >
        <generator class="assigned" />
    </id>

    <timestamp
        name="actualizacion"
        column="actualizacion"
    />

    <property
        name="descripcion"
        type="java.lang.String"
        column="descripcion"
        not-null="true"
        length="30"
    />
    <property
        name="regvigente"
        type="java.lang.String"
        column="regvigente"
        length="1"
    />

    <!-- associations -->

</class>
</hibernate-mapping>
